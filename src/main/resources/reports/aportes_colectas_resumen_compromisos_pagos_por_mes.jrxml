    <queryString language="SQL">
		<![CDATA[SELECT m.APELLIDOS, m.NOMBRES, m.CTACTE, c.MES, c.ANO, c.monto_compra, d.monto_pago FROM
 (SELECT ed.id_entidad,
        MONTH(ev.FECHA) AS mes,
        YEAR(ev.FECHA) AS ano,
        COALESCE(SUM(ed.monto),0) as monto_compra
      FROM MG.TBL_EVENTO_DETALLE ed
            LEFT JOIN MG.TBL_EVENTOS ev ON ed.ID_EVENTO = ev.ID WHERE (ev.ID_EVENTO_TIPO = 2 OR ev.ID_EVENTO_TIPO = 3)
            AND YEAR(ev.FECHA) = $P{pAno} AND MONTH(ev.FECHA) = $P{pMes}
        group by MONTH(ev.FECHA), YEAR(ev.FECHA), ed.id_entidad) c,
    (SELECT p.id_entidad,
        MONTH(p.fechahora_compromiso) AS mes,
        YEAR(p.fechahora_compromiso) AS ano,
        SUM(p.MONTO_APORTE + p.MONTO_DONACION) AS monto_pago
        FROM MG.TBL_TRANSFERENCIAS p WHERE (p.ID_EVENTO_TIPO = 2 OR p.ID_EVENTO_TIPO = 3)
        AND YEAR(p.fechahora_compromiso) = $P{pAno} AND MONTH(p.fechahora_compromiso) = $P{pMes}
        group by YEAR(p.fechahora_compromiso), MONTH(p.fechahora_compromiso), p.id_entidad
    UNION ALL
    SELECT p.id_entidad,
        MONTH(p.fechahora) AS mes,
        YEAR(p.fechahora) AS ano,
        -SUM(p.MONTO_APORTE + p.MONTO_DONACION) AS monto_pago
        FROM MG.TBL_RECIBOS p WHERE (p.ID_EVENTO_TIPO = 2 OR p.ID_EVENTO_TIPO = 3)
        AND YEAR(p.fechahora) = $P{pAno} AND MONTH(p.fechahora) = $P{pMes}
        group by YEAR(p.FECHAHORA), MONTH(p.FECHAHORA), p.id_entidad) d,
        MG.TBL_ENTIDADES m
where c.id_entidad = d.id_entidad AND (c.id_entidad = m.id OR c.id_entidad = m.ID_ENTIDAD_PAGANTE_APORTES)
AND c.ano = d.ano AND c.mes = d.mes
ORDER BY c.ano, c.mes, m.ctacte, m.APELLIDOS, m.NOMBRES]]>
    </queryString>
