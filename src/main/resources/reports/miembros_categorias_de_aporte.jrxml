	<queryString>
		<![CDATA[SELECT a.ID_ENTIDAD, a.ID_CATEGORIA_DE_PAGO, a.ANO_MES, 
e.APELLIDOS, e.NOMBRES, e.CTACTE, e.FECHA_BAUTISMO, e.FECHA_ENTRADA_CONGREGACION,
cp.DESCRIPCION FROM MG.TBL_ENTIDADES_HISTORICO_CATEGORIAS a
    INNER JOIN (SELECT ID_ENTIDAD, MAX(ANO_MES) ANO_MES FROM MG.TBL_ENTIDADES_HISTORICO_CATEGORIAS
    GROUP BY ID_ENTIDAD) b
    ON a.ID_ENTIDAD = b.ID_ENTIDAD AND a.ANO_MES = b.ANO_MES
    JOIN MG.TBL_MIEMBROS_CATEGORIAS_DE_PAGO cp ON a.ID_CATEGORIA_DE_PAGO = cp.ID
