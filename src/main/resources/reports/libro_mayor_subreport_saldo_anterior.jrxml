	<property name="com.jaspersoft.studio.data.defaultdataadapter" value="mg"/>
	<parameter name="ctaContable" class="java.lang.Integer"/>
	<parameter name="fechaDesde" class="java.util.Date"/>
	<queryString language="SQL">
		<![CDATA[SELECT SUM(S.SALDO_ANTERIOR) AS SALDO_ANTERIOR_SUMA FROM (
        SELECT A.MONTO AS SALDO_ANTERIOR
        FROM TBL_ASIENTOS A
        WHERE ID_CUENTA_CONTABLE_DEBE =  $P{ctaContable}
        AND A.FECHAHORA < $P{fechaDesde}
        UNION ALL
        SELECT -A.MONTO AS SALDO_ANTERIOR
        FROM TBL_ASIENTOS A
        WHERE ID_CUENTA_CONTABLE_HABER =  $P{ctaContable}
        AND A.FECHAHORA < $P{fechaDesde}
        ) AS S]]>
	</queryString>
	<field name="SALDO_ANTERIOR_SUMA" class="java.lang.Integer"/>
	<variable name="SALDO_ANTERIOR_SUMA" class="java.lang.Integer" calculation="Sum">
