	<parameter name="ctaContable" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT C.NRO, A.FECHAHORA, A.MONTO AS IMPORTE FROM TBL_ASIENTOS A, TBL_FACTURAS_COMPRA_ASIENTOS CA, TBL_FACTURAS_COMPRA C
WHERE C.ID = CA.ID_FACTURA_COMPRA AND A.ID = CA.ID_ASIENTO
AND ID_CUENTA_CONTABLE_DEBE =  $P{ctaContable} 
UNION ALL
SELECT C.NRO, A.FECHAHORA, -A.MONTO AS IMPORTE FROM TBL_ASIENTOS A, TBL_FACTURAS_COMPRA_ASIENTOS CA, TBL_FACTURAS_COMPRA C
WHERE C.ID = CA.ID_FACTURA_COMPRA AND A.ID = CA.ID_ASIENTO
AND ID_CUENTA_CONTABLE_HABER =  $P{ctaContable}
ORDER BY FECHAHORA]]>
