	<property name="com.jaspersoft.studio.data.defaultdataadapter" value="mg"/>
	<parameter name="id_evento" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT * FROM
(select COALESCE(SUM(rd.monto),0) as monto_compra, m.apellidos, m.nombres, m.ctacte, r.descripcion
    from tbl_eventos r, tbl_evento_detalle rd LEFT JOIN tbl_entidades m ON rd.id_entidad = m.id
    where rd.id_evento = r.id AND r.id =  $P{id_evento}
    GROUP BY m.id, m.apellidos, m.nombres, m.ctacte, r.descripcion) c,
(select id_entidad, SUM(monto_aporte + monto_donacion) as monto_pago FROM tbl_eventos r, (SELECT id as nro, fechahora, id_entidad, id_evento, monto_aporte, monto_donacion, 'Transferencia' AS tipo FROM tbl_transferencias
	UNION ALL
	SELECT id as nro, fechahora, id_entidad, id_evento, monto_aporte, monto_donacion, 'Efectivo' AS tipo  FROM tbl_recibos r
) as pagos LEFT JOIN  tbl_entidades m ON pagos.id_entidad = m.id
where pagos.id_evento = r.id AND r.id =  $P{id_evento}
GROUP BY pagos.id_entidad) p]]>
	</queryString>
